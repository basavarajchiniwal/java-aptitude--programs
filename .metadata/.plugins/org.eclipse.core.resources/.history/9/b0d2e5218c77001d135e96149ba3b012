package com.javatuts.basicprograms;

public class StarPattern {
	public static void main(String[] args) {
		int a = 7;
		for (int i = 0; i <= 7; i++) {
			for (int j = 2; j <= i; j++) {
				// if(i==j)
				System.out.print("* ");
			}
			System.out.println("\n");
			for (int l = 0; l <=7; l++) {
				System.out.print("* ");
			}
		}
	}
}
