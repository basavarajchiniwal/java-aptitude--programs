
package com.javatuts.basicprograms;

public class FibonacciSeries {

	public static void main(String[] args) {
int a=0;
int b=1;
int c;
System.out.println("Fibonacci Series Numbers");
System.out.println("\n"+a+"\n"+b+"\n");
for(int d=2;d<=10;d++) {
	c=a+b;
	System.out.println(c);
	a=b;
	b=c;
}
	}

}
